package SetsAndMaps;

import java.util.HashMap;
import java.util.Map;
import java.util.List;
import java.util.LinkedList;

/**
 *
 * @author zachboburka
 */
public class Agent {

    Map<String, String> mapAgent = new HashMap();
    List<String> list = new LinkedList<>();

    String idNum;
    String location;
    String name;
    String assignStat;
    String contactInfo;

    public void generateAgent() {

        mapAgent = new HashMap<>();
        list.add(idNum = "0001");
        list.add(location = "LA");
        list.add(name = "Joe Dirt");
        list.add(assignStat = "Compromised");
        list.add(contactInfo = "joedirt@gmail.com");

        mapAgent.put(idNum, "0001");
        mapAgent.put(location, "Los Angeles");
        mapAgent.put(name, "Joe Dirt");
        mapAgent.put(assignStat, "Compromised");
        mapAgent.put(contactInfo, "joedirt@gmail.com");

        System.out.println("");
        System.out.println(mapAgent.get(idNum));
        System.out.println(mapAgent.get(location));
        System.out.println(mapAgent.get(name));
        System.out.println(mapAgent.get(assignStat));
        System.out.println(mapAgent.get(contactInfo));
        System.out.println("");
        mapAgent.clear();

        mapAgent.put(idNum, "0002");
        mapAgent.put(location, "Khazikstan");
        mapAgent.put(name, "Borat");
        mapAgent.put(assignStat, "Successful");
        mapAgent.put(contactInfo, "borat@gmail.com");
        System.out.println("");
        System.out.println(mapAgent.get(idNum));
        System.out.println(mapAgent.get(location));
        System.out.println(mapAgent.get(name));
        System.out.println(mapAgent.get(assignStat));
        System.out.println(mapAgent.get(contactInfo));
        System.out.println("");
        mapAgent.clear();

        mapAgent.put(idNum, "0003");
        mapAgent.put(location, "The Britons");
        mapAgent.put(name, "King of The Britons");
        mapAgent.put(assignStat, "Successful");
        mapAgent.put(contactInfo, "holygrail@gmail.com");
        System.out.println("");
        System.out.println(mapAgent.get(idNum));
        System.out.println(mapAgent.get(location));
        System.out.println(mapAgent.get(name));
        System.out.println(mapAgent.get(assignStat));
        System.out.println(mapAgent.get(contactInfo));
        System.out.println("");
        mapAgent.clear();

        mapAgent.put(idNum, "0004");
        mapAgent.put(location, "Kentucky");
        mapAgent.put(name, "Colonel Sanders");
        mapAgent.put(assignStat, "Successful");
        mapAgent.put(contactInfo, "kfc@gmail.com");
        System.out.println("");
        System.out.println(mapAgent.get(idNum));
        System.out.println(mapAgent.get(location));
        System.out.println(mapAgent.get(name));
        System.out.println(mapAgent.get(assignStat));
        System.out.println(mapAgent.get(contactInfo));
        System.out.println("");
        mapAgent.clear();

        mapAgent.put(idNum, "0005");
        mapAgent.put(location, "Sunnyville Trailer Park");
        mapAgent.put(name, "Jim Lahey");
        mapAgent.put(assignStat, "Successful");
        mapAgent.put(contactInfo, "jimlahey@gmail.com");
        System.out.println("");
        System.out.println(mapAgent.get(idNum));
        System.out.println(mapAgent.get(location));
        System.out.println(mapAgent.get(name));
        System.out.println(mapAgent.get(assignStat));
        System.out.println(mapAgent.get(contactInfo));
        System.out.println("");
        mapAgent.clear();

    }//end generateAgent

