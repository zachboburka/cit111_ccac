package SetsAndMaps;

import java.util.HashMap;
import java.util.Map;

/**
 *
 * @author zachboburka
 */
public class Agent {

    Map<String, String> mapAgent = new HashMap<>();

    String idNum;
    String location;
    String name;
    String assignStat;
    String contactInfo;

    public void generateAgent() {

        mapAgent = new HashMap<>();
        mapAgent.put("IDNum", idNum);
        mapAgent.put("Location", location);
        mapAgent.put("Name", name);
        mapAgent.put("Assignment Status", assignStat);
        mapAgent.put("Contact Info", contactInfo);

        for (String key : mapAgent.keySet()) {
            System.out.println(key + " = " + mapAgent.get(key));
        }
    }//end generateAgent

}//end Agent
