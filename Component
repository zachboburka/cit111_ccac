package inheritance;

/**
 *
 * @author zachary.boburka
 */
public class Component {
    int year;
    int yearsAgo;
    String projectName;
    String author;
    String description;
    String type;

    //only pick author and description
    public int yearSince(int currentYear) {
        yearsAgo = (currentYear - year);

        return yearsAgo;
    }//end yearSince
}
